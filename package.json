{
  "name": "pageparser",
  "version": "1.1.8",
  "description": "A small cli web page parser",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "pageparser": "bin/cli"
  },
  "scripts": {
    "build": "gulp",
    "prepublish": "npm run build",
    "test": "mocha --require ts-node/register --require source-map-support/register --recursive --ui tdd ./tests/**/*.ts "
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/Serabass/pageparser"
  },
  "author": "Serabass",
  "license": "MIT",
  "dependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/deep-extend": "^0.4.31",
    "@types/es6-promise": "^3.3.0",
    "@types/request": "^2.47.0",
    "cheerio": "^1.0.0-rc.2",
    "deep-extend": "^0.5.0",
    "es6-promise": "^4.2.4",
    "iconv-lite": "^0.4.19",
    "minimist": "^1.2.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "stream-to-promise": "^2.2.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1",
    "gulp": "^3.9.1",
    "gulp-typescript": "^4.0.1"
  },
  "keywords": [
    "html",
    "parse",
    "selector",
    "jquery"
  ],
  "bugs": {
    "url": "https://github.com/Serabass/pageparser/issues"
  },
  "homepage": "https://github.com/Serabass/pageparser",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "source-map-support": "^0.5.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
